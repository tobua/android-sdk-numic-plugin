{
  "name": "android-sdk-numic-plugin",
  "description": "Configures native Android installation to match installed SDK.",
  "version": "0.0.0-development",
  "repository": "github:tobua/android-sdk-numic-plugin",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "build": "padua build",
    "start": "padua watch",
    "test": "vitest run"
  },
  "padua": {
    "esbuild": {
      "platform": "node",
      "format": "esm",
      "target": "node16"
    },
    "tsconfig": {
      "compilerOptions": {
        "target": "es2022"
      }
    }
  },
  "dependencies": {
    "semver-sort": "^1.0.0"
  },
  "peerDependencies": {
    "numic": ">= 0.9"
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "numic",
    "plugin",
    "android",
    "sdk"
  ],
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/semver-sort": "^0.0.1",
    "jest-fixture": "^4.0.2",
    "padua": "^1.3.1",
    "vitest": "^0.32.0"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs"
  },
  "publishConfig": {
    "provenance": true
  },
  "engines": {
    "node": ">= 16"
  }
}
